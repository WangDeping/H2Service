    <script>
        var CURRENT_URL = window.location.pathname.split('#')[0].split('?')[0]; 
       var expandLi= $("#m_ver_menu").find('a[href = "'+ CURRENT_URL + '"]').parent('li').parents('li').eq(0).addClass('m-menu__item--open').addClass('m-menu__item--expanded');
        var selectLi = $("#m_ver_menu").find('a[href = "' + CURRENT_URL + '"]').parent('li').addClass('m-menu__item--active');
       
        var array = new Array();
        array.push($.trim(expandLi.find('span').html()));
        array.push($.trim(selectLi.find('span').html()))
        array.forEach(function (name) {
            $('#subnav').append(' <li class="m-nav__separator">-</li>')
            $('#subnav').append('<li class="m-nav__item"><a href="" class="m-nav__link"><span class="m-nav__link-text" > ' + name + '</span ></a ></li >')
        });
    </script>
