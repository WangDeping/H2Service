
<div class="m-portlet">
    <div class="m-portlet__head">
        <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
                <span class="m-portlet__head-icon m--hide">
                    <i class="la la-gear"></i>
                </span>
                <h3 class="m-portlet__head-text">
                    实际就诊人次
                </h3>
            </div>
        </div>
    </div>   
   <div class="m-portlet__body">
            <div class="m-form__section m-form__section--first">
                <div class="form-group m-form__group">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class='input-group date' id='patQtyStart_datepicker'>
                                <input type='text' class="form-control m-input" readonly placeholder="开始日期" />
                                <span class="input-group-addon">
                                    <i class="la la-calendar-check-o"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class='input-group date' id='patQtyEnd_datepicker'>
                                <input type='text' class="form-control m-input" readonly placeholder="结束日期" />
                                <span class="input-group-addon">
                                    <i class="la la-calendar-check-o"></i>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group m-form__group">
                    <div class="row">
                        <div class="col-lg-8"></div>
                        <div class="col-lg-4">
                            <button id="queryPat_btn" onclick="search()" class="btn btn-primary m-btn m-btn--custom m-btn--icon m-btn--air ">
                                查询
                            </button>
                        </div>
                    </div>
                </div>
                <div class="form-group m-form__group">
                    <table class="bootstrap-table" id="patQty_grid" data-url='@Url.Action("GetOPQty")'
                           data-side-pagination="server" data-page-list="[10]" data-pagination="true"
                           data-single-select="true" data-striped="true">
                        <thead>
                            <tr>
                                <th data-field="Dep" data-align="center">科室</th>
                                <th data-field="Qty" data-align="center">数量</th>
                            </tr>
                        </thead>
                    </table>

                </div> 



            </div>
        </div>
   
</div>